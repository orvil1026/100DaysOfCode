   "source": [
    "def prime_checker(number):\n",
    "    \n",
    "    for i in range(2,number):\n",
    "        \n",
    "        if number%i==0:\n",
    "            print(f\"{number} is not a prime number.\")\n",
    "            return\n",
    "    print(f\"{number} is a prime number.\")    \n",
    "\n",
    "\n",
    "n = int(input(\"Check this number: \"))\n",
    "\n",
    "prime_checker(number=n)\n"
   ]
  }
